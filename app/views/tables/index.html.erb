<% @tables.each do |table| %>
  <div class="table">
    <%= table.name %>
    <%= link_to "Show all bookings", cafe_table_bookings_path(table) %>
    <%= link_to "Delete", cafe_table_path(table), data: { confirm: 'Are you sure?' }, method: :delete %>
    <%= link_to "Edit", edit_cafe_table_path(table) %>
    <%= link_to "Add booking", new_cafe_table_booking_path(table) %>
  </div>
<% end %>

